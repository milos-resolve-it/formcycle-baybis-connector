   <xs:complexType name="Code.Datenabruf.Nichterstellung.Grund">
      <xs:annotation>
         <xs:appinfo>
            <codeliste>
               <nameLang>Datenabruf nach § 34a BMG Nichterstellung Grund</nameLang>
               <nameKurz>Datenabruf nach § 34a BMG Nichterstellung Grund</nameKurz>
               <nameTechnisch>datenabruf.nichterstellung.grund</nameTechnisch>
               <kennung>urn:xoev-de:xmeld:codeliste:datenabruf.nichterstellung.grund</kennung>
               <beschreibung>Mit dieser Schlüsseltabelle werden für den Kontext der Datenabrufe nach § 34a BMG die Gründe definiert, die aufgrund rechtlicher und/oder verfahrenstechnischer Prüfungen zur Nichtbeantwortung einer Suchanfrage führen können.</beschreibung>
               <herausgebernameLang>Koordinierungsstelle für IT-Standards</herausgebernameLang>
               <herausgebernameKurz>KoSIT</herausgebernameKurz>
            </codeliste>
            <versionCodeliste>
               <version>2</version>
               <datumGueltigkeitAb>2023-11-01</datumGueltigkeitAb>
               <versionCodelistenHandbuch>1.2</versionCodelistenHandbuch>
            </versionCodeliste>
            <codelistenspalten>
               <key>
                  <datentyp>string</datentyp>
                  <codeSpalte>true</codeSpalte>
                  <verwendung>required</verwendung>
                  <empfohleneCodeSpalte>true</empfohleneCodeSpalte>
               </key>
               <name>
                  <datentyp>string</datentyp>
                  <codeSpalte>false</codeSpalte>
                  <verwendung>required</verwendung>
                  <empfohleneCodeSpalte>false</empfohleneCodeSpalte>
               </name>
            </codelistenspalten>
            <genutzteCodeSpalte>key</genutzteCodeSpalte>
         </xs:appinfo>
      </xs:annotation>
      <xs:complexContent>
         <xs:restriction base="xoev-code:Code">
            <xs:sequence>
               <xs:element name="code" form="unqualified">
                  <xs:simpleType>
                     <xs:restriction base="xs:token">
                        <xs:enumeration value="01">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Das in der Suchanfrage angegebene Identifikationsmerkmal ist ungültig.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="02">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Für die in den Auswahldaten angegebene Straße wird keine passende Straßenbezeichnung im Melderegister gefunden.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="03">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Im Element sicherheitsbehoerde wurde der Wert „true“ übermittelt, obwohl es sich tatsächlich nicht um eine Sicherheits- bzw. Strafermittlungsbehörde handelt.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="04">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Die abrufende Stelle ist zur Nutzung des Auswahldatums waffenrechtlicheErlaubnis nicht berechtigt.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="05">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Die abrufende Stelle ist zur Nutzung des Auswahldatums waffenbesitzverbot nicht berechtigt.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="06">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Die abrufende Stelle ist zur Nutzung des Auswahldatums sprengstoffrechtlicheErlaubnis nicht berechtigt.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="07">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Die abrufende Stelle ist zum Abruf von Daten zu Ausweis- und Passdokumenten nicht berechtigt.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="08">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Die abrufende Stelle ist zum Abruf von Tatsachen nach § 3 Absatz 2 Nummer 4 BMG zu den Pass- und Ausweisdaten nicht berechtigt.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="09">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Die abrufende Stelle ist zum Abruf von Daten für waffen- und sprengstoffrechtliche Verfahren nicht berechtigt.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="10">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Die abrufende Stelle ist zum Abruf von Daten zum Wohnungsgeber nicht berechtigt.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="11">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Die abrufende Stelle ist zum Abruf des Einzugsdatums nicht berechtigt.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="12">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Die Trefferliste überschreitet die maximale Größe von 1000 Treffern.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="13">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Die Ergebnisliste überschreitet die maximale Größe von 1000 Ergebnissen.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="14">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Die Angaben zum AGS des Lesers der Suchanfrage stimmen nicht mit dem AGS in den Auswahldaten überein.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="15">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Wenn in der Suchanfrage der Einsatz einer phonetischen Suche bestimmt ist, dürfen keine Platzhalter verwendet werden.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="16">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Es liegt ein interner technischer Fehler vor. Die Präzisierung erfolgt durch eine Freitextangabe im Element freitext.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="17">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Es liegt ein sonstiger fachlicher Fehler vor. Die Präzisierung erfolgt durch eine Freitextangabe im Element freitext.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="18">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>In den Auswahldaten sind die Mindestangaben gemäß § 38 Abs. 1 BMG für die Personensuche nach § 34a Abs. 2 BMG nicht erfüllt.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="19">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>In dem in den Auswahldaten der Suchanfrage angegebenen Hausnummernbereich ist die Ende-Angabe kleiner als die oder gleich der Beginn-Angabe.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="20">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>In dem in den Auswahldaten der Suchanfrage angegebenen Geburtszeitraum liegt das Ende-Datum am oder vor dem Beginn-Datum.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="21">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>In den Auswahldaten der Suchanfrage wurde auf aktuelle oder inaktuelle Anschriften eingeschränkt, jedoch für die Anschrift keine Straße angegeben.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="22">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Der in der Suchanfrage bestimmte Suchbereich liegt nicht im Zuständigkeitsbereich der angefragten Stelle.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="23">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Das Element landesspezifischeErweiterungen wurde im länderübergreifenden Datenabruf übermittelt.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="24">
                           <xs:annotation>
                              <xs:appinfo>
                                 <name>Die AZR-Nummer ist in dieser Anfrage als Auswahldatum unzulässig.</name>
                              </xs:appinfo>
                           </xs:annotation>
                        </xs:enumeration>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:element>
            </xs:sequence>
            <xs:attribute name="listURI"
                          type="xs:anyURI"
                          use="optional"
                          fixed="urn:xoev-de:xmeld:codeliste:datenabruf.nichterstellung.grund"/>
            <xs:attribute name="listVersionID"
                          type="xs:normalizedString"
                          use="optional"
                          fixed="2"/>
         </xs:restriction>
      </xs:complexContent>
   </xs:complexType>
