   <xs:complexType name="Code.Gebiet">
      <xs:annotation>
         <xs:appinfo>
            <codeliste>
               <nameLang>Codeliste Staatsgebiete aus der Staats- und Gebietssystematik des Statistischen Bundesamtes</nameLang>
